      "type": "table",
      "id": 8,
      "title": "Last Update Age",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "time() - pulumi_stack_last_update_timestamp{org=~\"$org\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "dtdurations",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 86400 },
              { "color": "red", "value": 604800 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "org" },
            "properties": [{ "id": "custom.width", "value": 120 }]
          },
          {
            "matcher": { "id": "byName", "options": "project" },
            "properties": [{ "id": "custom.width", "value": 150 }]
          },
          {
            "matcher": { "id": "byName", "options": "stack" },
            "properties": [{ "id": "custom.width", "value": 120 }]
          },
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [
              { "id": "displayName", "value": "Age" },
              { "id": "unit", "value": "dtdurations" }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Age", "desc": false }],
        "cellHeight": "sm",
        "footer": { "show": false }
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true
            },
            "indexByName": {},
            "renameByName": {
              "Value": "Age"
            }
          }
        }
      ]
    },
